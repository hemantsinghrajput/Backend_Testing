{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./functions", // Changed from "dist" to "functions"
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["server.ts", "routes/**/*", "utils/**/*", "models/**/*", "lib/**/*", "types/**/*", "landingfeeds.ts"]
}