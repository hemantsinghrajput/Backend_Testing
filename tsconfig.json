{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./",                  
    "outDir": "./dist",               // <--- FIXED from "functions" to "dist"
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "server.ts",
    "routes/**/*",
    "utils/**/*",
    "models/**/*",
    "lib/**/*",
    "types/**/*",
    "landingfeeds.ts"
  ],
  "exclude": ["node_modules", "dist"]  // <--- exclude dist instead of functions
}
